<h1>Day Scores</h1>

<div id="day-scores">
  <table class="table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Tension</th>
        <th>Sleep</th>
      </tr>
    </thead>
    <tbody>
      <tr v-for="score in day_scores">
        <td>{{ score.day }}</td>
        <td>{{ score.tension }}</td>
        <td>{{ score.sleep }}</td>
      </tr>
    </tbody>
  </table>

